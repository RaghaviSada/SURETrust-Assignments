pair<long long, long long> getMinMax(long long a[], int n) {
    
    long long min=INT_MAX;
    long long max=INT_MIN;
    for(int i=0;i<n;i++)
    {
        if(a[i] > max)
        {
            max=a[i];
        }
    }
    for(int i=0;i<n;i++)
    {
        if(a[i]< min)
        {
            min=a[i];
        }
    }

    pair<long long , long long>ans;
    ans.first=min;
    ans.second=max;
    return ans;
}
